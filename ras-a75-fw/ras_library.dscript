const addr_row_DAC_high = 0x8C
const addr_row_DAC_low = 0x88
const addr_cluster_DAC_high = 0x94
const addr_cluster_DAC_low = 0x90

function rampe_DAC(addr){
//sendToRawAddressing_stopRunning()  
for (i = 0 ; i < 4096 ; i++){
//sendToRawAddressing_stopRunning()  
sendToRawAddressing_writeReadRegister(addr,i,addr)
sendToRawAddressing_writeReadRegister(0x98,1,0x98)
//sendToRawAddressing_startRunning() 
}
//sendToRawAddressing_startRunning()  
}

function all_ramp_DAC(pas){
    for (i = 0 ; i < 4096/pas ; i++){
        sendToRawAddressing_stopRunning()
        sendToRawAddressing_writeReadRegister(addr_row_DAC_high,i*pas,addr_row_DAC_high)
        sendToRawAddressing_writeReadRegister(addr_row_DAC_low,i*pas,addr_row_DAC_low)
        sendToRawAddressing_writeReadRegister(addr_cluster_DAC_high,i*pas,addr_cluster_DAC_high)
        sendToRawAddressing_writeReadRegister(addr_cluster_DAC_low,i*pas,addr_cluster_DAC_low)
        sendToRawAddressing_writeReadRegister(0x98,1,0x98)
        sendToRawAddressing_startRunning()
    }
}

